<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>net.reinteractive.spanner-cleaner</string>
    <key>Program</key>
    <string>{{ home_folder }}/.cache/spanner/cleaner.sh</string>
    <key>KeepAlive</key>
    <dict>
      <key>PathState</key>
      <dict>
        <key>{{ home_folder }}/.cache/spanner/authorisations</key>
        <true/>
      </dict>
    </dict>
    <key>ThrottleInterval</key>
    <integer>3600</integer>
  </dict>
</plist>
